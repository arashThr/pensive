{
    "manifest_version": 3,
    "name": "Bookmark to API",
    "version": "1.0",
    "description": "Sends bookmarked URLs and tags to a REST endpoint.",
    "permissions": [
      "bookmarks",
      "storage",
      "activeTab"
    ],
    "host_permissions": [
      "http://localhost:8000/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html"
  }